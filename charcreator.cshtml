    //check user to enable or disable readonly (imitation cookie is created for testing purposes)
    if (Session["username"] != null)
    {
        username = (string)Session["username"];
    }
    else
    {
        Response.Redirect(@Href("~/Loginpage.cshtml"));
    }

    //if provided a character id, load character from db

    Character character = new Character();
    var charid = "";
    if (Request.Params["characterid"] != null)
    {
        charid = Request.Params["characterid"];
        character = DatabaseManagement.getCharacter(Int32.Parse(charid));
        charname = character.getName();
        charclass = character.getClass();
        race = character.getRace();
        sex = character.getGender();
        alignment = character.getAlignment();
        background = character.getBackground();
        level = character.getLevel().ToString();
        exp = character.getEXP().ToString();
        age = character.getAge().ToString();
        eyes = character.getEyes();
        skin = character.getSkin();
        hair = character.getHair();

        strength = character.getStrength().ToString();
        dex = character.getDex().ToString();
        intelligence = character.getIntelligence().ToString();
        speed = character.getSpeed().ToString();
        inspiration = character.getInspiration().ToString();
        constitution = character.getConstitution().ToString();
        wisdom = character.getWisdom().ToString();
        charisma = character.getCharisma().ToString();

